{
  "packageManager": "yarn@3.2.3",
  "name": "kkogkkog-frontend",
  "version": "0.1.0",
  "sideEffects": false,
  "scripts": {
    "start": "cross-env NODE_ENV=local webpack serve --config ./config/webpack.dev.js",
    "start:dev": "cross-env NODE_ENV=development webpack serve --config ./config/webpack.dev.js",
    "start:prod": "cross-env NODE_ENV=production webpack serve --config ./config/webpack.prod.js",
    "build": " cross-env NODE_ENV=production webpack --progress --config ./config/webpack.prod.js",
    "build:dev": "cross-env NODE_ENV=development BABEL_ENV=production webpack --progress --config ./config/webpack.prod.js",
    "build:local": " cross-env NODE_ENV=local webpack --progress --config ./config/webpack.dev.js",
    "test": "jest --watchAll",
    "coverage": "jest --coverage"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-query": "^3.39.1",
    "react-router-dom": "6"
  },
  "devDependencies": {
    "@babel/core": "^7.18.6",
    "@babel/preset-env": "^7.18.6",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@emotion/babel-plugin": "^11.10.2",
    "@emotion/core": "^11.0.0",
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.7",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.3.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^14.4.3",
